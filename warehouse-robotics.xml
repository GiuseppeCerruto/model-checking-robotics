<nta>
	<declaration>// Place global declarations here.
const int N = 10;
const int M = 10;
const int P = 20;

typedef struct {
bool isPodOn;
bool isRobotOn;
bool isEntry;
bool isDelivery;
bool will_be_occupied;
} Square;

Square matrix[N][M]; 


typedef struct {
int x;
int y;
bool assigned;
} Pod;

Pod pods[P];


chan pick, release;</declaration>
	<template>
		<name>TaskDispatcher</name>
		<parameter>int T, int MAX_T</parameter>
		<declaration>clock x;</declaration>
		<location id="id0" x="-1470" y="-442">
			<name x="-1480" y="-476">initial</name>
		</location>
		<location id="id1" x="-1360" y="-314">
			<label kind="invariant" x="-1370" y="-297">x&lt;=T</label>
		</location>
		<location id="id2" x="-1156" y="-314">
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id1"/>
			<target ref="id1"/>
			<nail x="-1445" y="-323"/>
			<nail x="-1419" y="-263"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="-1275" y="-306">x&gt;=T</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="-1462" y="-382">x=0</label>
		</transition>
	</template>
	<template>
		<name x="5" y="5">Robot</name>
		<declaration>// Place local declarations here.

</declaration>
		<location id="id3" x="-1079" y="-348">
			<name x="-1089" y="-382">initial</name>
		</location>
		<init ref="id3"/>
	</template>
	<template>
		<name>Human</name>
		<parameter>int H</parameter>
		<declaration>clock x;
</declaration>
		<location id="id4" x="-450" y="-204">
			<name x="-460" y="-238">idle</name>
		</location>
		<location id="id5" x="-289" y="-85">
			<name x="-297" y="-68">picking</name>
			<label kind="invariant" x="-263" y="-93">x&lt;=H</label>
		</location>
		<init ref="id4"/>
		<transition>
			<source ref="id5"/>
			<target ref="id4"/>
			<label kind="guard" x="-307" y="-174">x&gt;=H</label>
			<label kind="synchronisation" x="-314" y="-195">release!</label>
			<nail x="-316" y="-174"/>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="-433" y="-68">pick?</label>
			<label kind="assignment" x="-433" y="-85">x=0</label>
			<nail x="-399" y="-93"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
r = Robot();
h = Human(10);
// List one or more processes to be composed into a system.
system r, h;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
